   <DataTemplate x:Key="editingRowGlyph">
      <Grid Height="15"
            Width="25"
            VerticalAlignment="Center"
            HorizontalAlignment="Center"
            Margin="3">
         <Border x:Name="WhiteBorder"
                 BorderThickness="0,0,1,1"
                 CornerRadius="4"
                 Margin="1"
                 BorderBrush="White" />
         <Border x:Name="BlackBorder"
                 BorderThickness="0,0,1,1"
                 CornerRadius="5"
                 Margin="1,1,0,0"
                 BorderBrush="Black" />
         <TextBlock Text="abc"
                    FontSize="9"
                    Foreground="White"
                    VerticalAlignment="Center"
                    HorizontalAlignment="Center"
                    Margin="1,0,0,0"
                    FontFamily="Tahoma" />
      </Grid>
   </DataTemplate>

